   "source": [
    "# Generate frames\n",
    "for s in np.unique(df[\"SOD\"]):\n",
    "    print(s)\n",
    "    sub_df = df[df[\"SOD\"] == s]    \n",
    "    spec_graph = alt.Chart(sub_df).mark_rect(strokeWidth=3).encode(\n",
    "            x=alt.X(\"Longtidue:O\",axis=alt.Axis(labels=False,ticks=False,grid=False,title=\"\")),\n",
    "            y=alt.Y(\"Negative_Latitude:O\",sort=\"descending\",axis=alt.Axis(labels=False,ticks=False,grid=False,title=\"\")),\n",
    "            color=alt.Color('STEC:Q',scale=alt.Scale(scheme='plasma'),legend=None),\n",
    "            fill=alt.Fill('STEC:Q',scale=alt.Scale(scheme='plasma'),legend=None),\n",
    "            stroke=alt.Fill('STEC:Q',scale=alt.Scale(scheme='plasma'),legend=None),\n",
    "        ).properties(height=360,width=360)+back_ground\n",
    "    spec_graph.save(\"geo_images/grid_\"+str(s)+\".png\")"
   ]
  },
  {
