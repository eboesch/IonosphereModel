   "source": [
    "from vega_datasets import data\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import altair as alt\n",
    "import cv2\n",
    "import os\n",
    "alt.data_transformers.disable_max_rows()\n"
    "os.makedirs("outputs/", exist_ok = True)\n"
    "os.makedirs("geo_images/", exist_ok = True)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "4cfbfe80",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load data\n",
    "countries = alt.topo_feature(data.world_110m.url, 'countries')\n",
    "back_ground = alt.Chart(countries).mark_geoshape(\n",
    "    fill='lightgray',\n",
    "    stroke='white',\n",
    "    opacity=0.25\n",
    ").project(\n",
    "    \"mercator\"\n",
    ")\n",
    "\n",
    "df = pd.read_csv(\"global_inference.csv\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "42f15c75",
   "metadata": {},
   "outputs": [],
   "source": [
    "not_corner_mask = np.logical_or(df[\"Latitude\"] > 0,df[\"Longtidue\"] > 0)\n",
    "df[\"STEC_test\"] = np.array(df[\"STEC\"])*not_corner_mask"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "0e652cbd",
   "metadata": {},
   "outputs": [],
   "source": [
    "df[\"Negative_Latitude\"] = -np.array(df[\"Latitude\"])"
   ]
  },
